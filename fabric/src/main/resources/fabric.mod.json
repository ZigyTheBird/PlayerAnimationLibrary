{
  "schemaVersion": 1,
  "id": "player_animation_library",
  "version": "${version}",
  "name": "Player Animation Library",
  "description": "Library that allows mods and data packs to animate the player.",
  "authors": [
    "Zigy"
  ],
  "contact": {
    "sources": "https://github.com/ZigyTheBird/PlayerAnimationLibrary"
  },
  "license": "MIT License",
  "icon": "assets/player_animation_library/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.zigythebird.playeranim.fabric.ModInitFabric"
    ],
    "client": [
      "com.zigythebird.playeranim.fabric.client.ModInitFabricClient"
    ]
  },
  "mixins": [
    "player_animation_library.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.10",
    "minecraft": "~1.21.4",
    "java": ">=21",
    "fabric-api": "*"
  }
}
